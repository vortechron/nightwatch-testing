{
    "name": "vortechron/nightwatch-testing",
    "description": "Laravel package for testing Nightwatch monitoring events with comprehensive variations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amirul Adli",
            "email": "amiruladli@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/mail": "^11.0|^12.0",
        "illuminate/notifications": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vortechron\\NightwatchTesting\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vortechron\\NightwatchTesting\\NightwatchTestingServiceProvider"
